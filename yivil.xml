<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote  name="github"
           fetch="ssh://git@github.com" />
  <remote  name="oe"
           fetch="git://git.openembedded.org" />
  <remote  name="yp"
           fetch="git://git.yoctoproject.org" />
  <default revision="master"
           remote="yp" />

  <notice>
repo init -m (null).xml is no longer supported, nor expected to fully
work.
  </notice>

<project path="yivil" name="hbrns/yivil" remote="github" />

<project path="yivil/.oe-core" name="openembedded-core" remote="oe" />
<project path="yivil/.meta-openembedded" name="meta-openembedded" remote="oe" />
<project path="yivil/bitbake" name="bitbake" remote="oe" />

<project path="yivil/meta-ivi" name="meta-ivi" remote="yp" />
<project path="yivil/meta-systemd" name="meta-systemd" remote="yp" />

<project path="yivil/meta-ti" name="meta-ti" remote="yp" />
<project path="yivil/meta-raspberrypi" name="djwillis/meta-raspberrypi" remote="github" />

</manifest>
